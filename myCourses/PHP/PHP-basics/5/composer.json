{
    "name": "motoyama/5-project",
    "description": "A web application skeleton with Twig template engine.",
    "require": {
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Composer\\Config::disablePlugins"
        ]
    }
}
